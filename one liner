waybackurl domain | tee file_wayback | grep -E "\.(js|py|xml|xls|pptx|asp|txt|.bin|.cfg|.secret|.sql|.sqld|.backup|.config|.pdf)|[a-zA-Z0-9_\.+-]+(@|%40)(gmail|yahoo|hotmail|protonmail|outlook)\.(com|me)|ey?[a-zA-Z0-9_](==|%3D%3D|%3D)$" | tee juicy
$(pwd | awk '{ print $1 }')" -type f -name "*.zip" -exec sh -c 'for file; do if [ -w "$file" ]; then rm -f "$file"; fi; done' sh {} + 
subfinder -d domain | httprobe | tee live_ | httpx -sc -td -title -ip -method -o httpx_